.con-up{
  background-color: #0b9ad6;
  justify-content: center; 
  text-align: center;
  padding: 20rpx 0
}
.con-up-tx{
  width: 232rpx;
  height: 232rpx;
  margin: 0 auto;
}
.con-up-tx image{
  width:100%;
  height: 100%;
  background-color: #000;
  border-radius: 50%;
}
.con-up-yhm{
  font-size: 48rpx;
  color: #fff
}
.con-up-hy{
   width: 212rpx;
   height: 64rpx;
   line-height: 64rpx;
   background-color: #fff;
   color: #666;
   border-radius: 25rpx;
   margin: 10rpx auto 0 auto;
}

.con-cen{
  width:94%;
  padding: 20rpx 3%;
  background-color: #fff
}
.con-cen-list{
  display: flex;
  justify-content: space-between;
  font-size: 36rpx;
  padding: 10rpx 0;
}
.cen-list-img image{
  width: 88rpx;
  height: 88rpx;
}
.cen-list-text{
  border-bottom: 1px solid #ccc;
  width: 1200rpx;
  line-height: 78rpx;
  margin-left: 42rpx;
  padding: 10rpx 0;
  position: relative;
}
.cen-list-text image{
  width: 24rpx;
  height: 44rpx;
  position: absolute;
  right: 0;
  top: 28rpx;
}