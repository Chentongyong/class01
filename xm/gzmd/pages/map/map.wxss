.topBox{
  width: 100%;
  background-color: #fff;
  font-size: 28rpx
}
.topBox .top-ss{
  padding: 24rpx 74rpx;
}
input{
  width: 100%;
  border: 1px solid #9B9B9B;
  text-align: center;
  border-radius: 25rpx;
  /* padding: 28rpx 0; */
  height: 82rpx
}
.topBox .top-map{
  width: 94%;
  padding: 24rpx 3%;
}
.topBox .top-map .present{ 
  font-size: 36rpx;
  color: #f10707;
}
.map-list{
  width: 100%;
  background-color: #fff;
  text-indent: 40rpx;
}
.map-list .map-list-zi{
  padding-bottom: 12rpx;
  font-size: 28rpx;
}
.map-list .text{
  display: block;
  width: 100%;
  height: 54rpx;
  line-height: 54rpx;
  background-color: #e6e6e6;
}
.map-list .map-list-zi view text{
  display: block;
  height: 82rpx;
  line-height: 82rpx;
  border-bottom: 1px solid #e6e6e6
}
.ssBox{
  border-top: 1px solid #ccc;
}
.ss{
  width: 100%;
  padding: 20rpx 0;
  border-bottom: 1px solid #ccc;
  background-color:#fff; 
  text-align: center;
  font-size: 28rpx;
  color: #666
}